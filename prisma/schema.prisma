generator client {
  provider = "prisma-client-js"
  output   = "/node_modules/@prisma/client/"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        Int      @id
  name      String
  email     String   @unique(map: "usuario_email_key")
  senha     String
  createdAt DateTime @default(now()) @db.Timestamp(6)
}
